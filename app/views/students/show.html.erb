<p style="color: green"><%= notice %></p>

<div class="ms-2">
  <%= render @student %>
</div>

<div class="p-3">
  <div class="p-3">
    <%= link_to "Voltar", students_path, :class => "btn btn-primary" %>
    <%= link_to "Editar", edit_student_path(@student), :class => "btn btn-primary" %>
  </div>
  <%= button_to "Apagar Estudante", @student, method: :delete, :class => "btn btn-danger" %>
</div>
